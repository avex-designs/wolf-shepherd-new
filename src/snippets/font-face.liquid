<link href="https://fonts.googleapis.com/css?family=Poppins:400,600|Roboto:300" rel="stylesheet">

<style>
{% comment %} Font-Regular {% endcomment %}
{% comment %} 
@font-face {
  font-family: 'Title';
  src:  url("{{ 'Font-Regular.eot' | asset_url }}") format('embedded-opentype'),  
        url("{{ 'Font-Regular.otf' | asset_url }}")  format('opentype'),
	      url("{{ 'Font-Regular.woff' | asset_url }}") format('woff'), 
        url("{{ 'Font-Regular.ttf' | asset_url }}")  format('truetype'), 
        url("{{ 'Font-Regular.svg' | asset_url }}") format('svg');
  font-weight: normal;
  font-style: normal;
} 
{% endcomment %}

{% comment %} Font-RegularItalic {% endcomment %}
{% comment %} 
