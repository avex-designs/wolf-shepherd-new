  font-family: 'Title';
  src:  url("{{ 'Font-RegularItalic.eot' | asset_url }}") format('embedded-opentype'),  
        url("{{ 'Font-RegularItalic.otf' | asset_url }}")  format('opentype'),
	      url("{{ 'Font-RegularItalic.woff' | asset_url }}") format('woff'), 
        url("{{ 'Font-RegularItalic.ttf' | asset_url }}")  format('truetype'), 
        url("{{ 'Font-RegularItalic.svg' | asset_url }}") format('svg');
  font-weight: normal;
  font-style: italic;
} 
{% endcomment %}


{% comment %} Font-Regular {% endcomment %}
{% comment %} 
